{
  "version": "1.0.0",
  "frameworks": {
    "net451": { },
    "netcoreapp1.0": {
      "imports": [
        "portable-net451+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0-*",
          "type": "platform"
        }
      }
    }
  },
  "dependencies": {
    "Demo.StartWars": "1.0.0",
    "Lib.AspNetCore.Mvc.JqGrid.DataAnnotations": "1.0.0-beta",
    "Lib.AspNetCore.Mvc.JqGrid.Core": "1.0.0-beta",
    "Lib.AspNetCore.Mvc.JqGrid.Helper": "1.0.0-beta",
    "Microsoft.AspNetCore.Mvc": "1.0.1",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.1",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0",
    "Microsoft.Extensions.Configuration": "1.0.0",
    "Microsoft.Extensions.Configuration.FileExtensions": "1.0.0"
  },
  "runtimeOptions": {
    "gcServer": true,
    "configProperties": {
      "System.GC.Server": true
    }
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true
  },
  "publishOptions": {
    "include": [
      "Views",
      "web.config",
      "wwwroot"
    ]
  }
}
